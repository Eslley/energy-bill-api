{
  "name": "energy-bill-api",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --transpile-only -r tsconfig-paths/register src/main.ts",
    "prestart": "prisma migrate deploy",
    "start": "node dist/main.js",
    "prebuild": "rimraf dist",
    "build": "swc src -d dist",
    "test": "cross-env NODE_ENV=test jest -c jest.config.json",
    "test:watch": "cross-env NODE_ENV=test jest -c jest.config.json --watch",
    "test:coverage": "cross-env NODE_ENV=test jest -c jest.config.json --coverage",
    "test:unit": "cross-env NODE_ENV=test jest -c test/jest.unit.config.json",
    "test:e2e": "cross-env NODE_ENV=test jest -c test/jest.e2e.config.json",
    "test:bug": "cross-env NODE_ENV=test jest -c test/jest.bug.config.json",
    "format:check": "prettier --check \"(src|test)/**/*.ts\"",
    "format:write": "prettier --write \"(src|test)/**/*.ts\"",
    "lint:check": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:write": "eslint --fix \"src/**/*.ts\" \"test/**/*.ts\"",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy": "prisma migrate deploy",
    "generate": "prisma generate",
    "make:use-case": "plop --plopfile ./.plop/use-case.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
