    "express": "^4.18.2",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "pino": "^8.11.0",
