    "@types/jsonwebtoken": "^9.0.1",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.5",
    "@types/pg": "^8.6.6",
    "@types/prettier": "^2.7.2",
